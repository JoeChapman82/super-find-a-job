{
  "name": "super-find-a-job",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "devstart": "gulp",
    "local-setup": "cp ./dev/env/local/{.env.testing,ecosystem.config.js} .",
    "test": "nyc --reporter=lcovonly --reporter=text mocha ./test/test_setup.js ./test/unit --recursive --exit",
    "testing": "nodemon --exec 'nyc mocha ./test/test_setup.js ./test/unit --recursive --exit'",
    "unit-test": "nyc mocha ./test/test_setup.js ./test/unit --recursive --exit",
    "unit-testing": "nodemon --exec 'nyc mocha ./test/test_setup.js ./test/unit --recursive --exit'",
    "integration-test": "nyc mocha ./test/test_setup.js ./test/integration --recursive --exit",
    "integration-testing": "nodemon --exec 'nyc mocha ./test/test_setup.js ./test/integration --recursive --exit'",
    "scan": "sonar-scanner -Dsonar.host.url=$SONAR_HOST -Dsonar.projectVersion=$npm_package_version",
    "create-vprops": "node ./properties/populateProperties.js",
    "prebuild": "cp ./dev/env/dev/.env.testing . && npm cit",
    "build": "npm run create-vprops && npm run scan && npm prune",
    "postbuild": "npm publish && npm pack",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "release": "release-it patch --ci",
    "postrelease": "release-it patch --preRelease=SNAPSHOT --ci"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.9.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "faker": "^5.5.3",
    "govuk-frontend": "^3.11.0",
    "helmet": "^3.16.0",
    "jsonwebtoken": "^8.5.1",
    "nunjucks": "^3.2.0",
    "properties-reader": "0.0.16",
    "require-dir": "^1.2.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "chai": "^4.2.0",
    "gulp": "^4.0.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.2",
    "gulp-uglify-es": "^2.0.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^15.0.0",
    "pump": "^3.0.0",
    "sonar-scanner": "^3.1.0"
  },
  "author": "",
  "license": "ISC"
}
